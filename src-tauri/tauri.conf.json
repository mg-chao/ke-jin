{
    "$schema": "../node_modules/@tauri-apps/cli/schema.json",
    "build": {
        "beforeBuildCommand": "npm run build && npm run export",
        "beforeDevCommand": "npm run dev",
        "devPath": "http://localhost:3001",
        "distDir": "../out"
    },
    "package": {
        "productName": "可今",
        "version": "0.1.0"
    },
    "tauri": {
        "allowlist": {
            "fs": {
                "all": true,
                "scope": ["$CONFIG/**"]
            }
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": ["icons/32x32.png", "icons/128x128.png", "icons/icon.ico"],
            "identifier": "com.kejin.test",
            "longDescription": "",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 720,
                "center": true,
                "resizable": true,
                "title": "可今",
                "width": 1280
            }
        ],
        "systemTray": { "iconPath": "./icons/icon.ico" }
    }
}
